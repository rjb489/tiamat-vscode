{
  "name": "tiamat-vscode",
  "displayName": "Tiamat",
  "description": "LLM-based conversational agent intended for use by novice programming students. ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pehilbert/tiamat-vscode"
  },
  "contributes": {
    "commands": [
      {
        "command": "tiamat-vscode.helloWorld",
        "title": "Tiamat: Hello World"
      },
      {
        "command": "tiamat-vscode.chat",
        "title": "Tiamat: Open Chat"
      }
    ]
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:react",
    "build:ts": "tsc -p ./",
    "build:react": "npx webpack --config webpack.config.js",
    "watch": "concurrently \"npm run watch:ts\" \"npm run watch:react\"",
    "watch:ts": "tsc -w",
    "watch:react": "npx webpack --watch --config webpack.config.js",
    "vscode:prepublish": "npm run build",
    "compile": "npm run build",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.1",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^4.0.0",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.2.1",
    "axios": "^1.7.7",
    "express": "^4.21.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "tiamat-vscode": "file:"
  }
}
